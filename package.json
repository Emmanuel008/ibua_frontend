{
  "name": "cardsjs",
  "private": true,
  "version": "0.0.0",
  "dependencies": {
    "framer-motion": "^12.23.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-rotating-text": "^1.4.1",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "postinstall": "node -e \"const fs=require('fs');const path=require('path');const d=path.join(process.cwd(),'src','node_modules');const t=path.join(process.cwd(),'node_modules','react-refresh');if(!fs.existsSync(d))fs.mkdirSync(d,{recursive:true});const link=path.join(d,'react-refresh');try{if(!fs.existsSync(link))fs.symlinkSync(path.relative(d,t),link,'dir');}catch(e){if(e.code!=='EEXIST')throw e;}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
